{
  "name": "eRejestracja",
  "description": "eRejestracja",
  "version": "1.0.0",
  "contributors": [
    "Tomasz Tomys <tomasz@tomys.pl> (http://tomys.pl)",
    "Dariusz Paluch <dariusz.paluch@hotmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tomasztomys/eRejestracja"
  },
  "bugs": {
    "url": "https://github.com/tomasztomys/eRejestracja/issues"
  },
  "config": {
    "port": 3000
  },
  "scripts": {
    "clean": "rimraf build/ *.log",
    "build": "npm run clean && webpack --config webpack/config.build.js",
    "dev": "npm run clean && webpack-dev-server --config webpack/config.dev.js",
    "serve": "npm run clean && webpack-dev-server --config webpack/config.dev.js",
    "test": "jest",
    "flow": "flow check --color always --show-all-errors",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint src/scripts/**/*",
    "lint:sass": "sass-lint -v src/styles/**/*"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.0",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0",
    "babel-jest": "^10.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.0",
    "eslint": "^2.5.0",
    "eslint-plugin-react": "^4.2.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.0",
    "flexboxgrid": "^6.3.0",
    "flow-bin": "^0.22.0",
    "html-webpack-plugin": "^2.15.0",
    "immutable": "^3.8.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^0.10.0",
    "json-loader": "^0.5.0",
    "node-sass": "^3.4.0",
    "postcss-loader": "^0.8.0",
    "qs": "^6.1.0",
    "react-dom": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "react-toolbox": "^0.16.2",
    "redux": "^3.0.5",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^2.0.1",
    "resolve-url-loader": "^1.4.0",
    "rimraf": "^2.5.0",
    "sass-lint": "^1.5.0",
    "sass-loader": "^3.2.0",
    "store": "^1.3.20",
    "style-loader": "^0.13.0",
    "svg-inline-react": "^1.0.1",
    "webpack": "^1.12.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^2.0.0"
  }
}
